<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #07C6C6;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkfx.SendMessageController">
   <children>
      <Button fx:id="sendMessageButton" layoutX="185.0" layoutY="256.0" mnemonicParsing="false" onAction="#sendButtonOnAction" prefHeight="33.0" prefWidth="231.0" style="-fx-background-color: #CAF4F4; -fx-background-radius: 30 30 30 30;" text="Send" textFill="#0b068f">
         <font>
            <Font name="Serif Regular" size="15.0" />
         </font>
      </Button>
      <TextField fx:id="toTextField" layoutX="152.0" layoutY="96.0" prefHeight="25.0" prefWidth="291.0" promptText="To" />
      <TextField fx:id="messageTextField" layoutX="152.0" layoutY="153.0" prefHeight="25.0" prefWidth="291.0" promptText="Message" />
      <Text fill="#0b068f" layoutX="152.0" layoutY="65.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Send a message">
         <font>
            <Font name="Vivaldi Italic" size="40.0" />
         </font>
      </Text>
      <Button fx:id="backButton" layoutX="182.0" layoutY="314.0" mnemonicParsing="false" onAction="#goBack" prefHeight="33.0" prefWidth="231.0" style="-fx-background-color: #CAF4F4; -fx-background-radius: 30 30 30 30;" text="Back" textFill="#0b068f">
         <font>
            <Font name="Serif Regular" size="15.0" />
         </font>
      </Button>
      <Label fx:id="sentLabel" layoutX="284.0" layoutY="208.0" />
      <Label fx:id="yourUsernameLabel" layoutX="28.0" layoutY="26.0" text="Label" textFill="#07c6c6" />
   </children>
</AnchorPane>
