<AnchorPane prefHeight="522.0" prefWidth="915.0" style="-fx-background-color: #CAF4F4;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.socialnetworkfx.YourRequestsController">
   <children>
