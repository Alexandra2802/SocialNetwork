         <children>
            <Button fx:id="leaveButton" layoutX="49.0" layoutY="399.0" mnemonicParsing="false" onAction="#leaveButtonOnAction" prefHeight="33.0" prefWidth="231.0" style="-fx-background-color: #CAF4F4; -fx-background-radius: 30 30 30 30;" text="Back" textFill="#0b068f">
               <font>
                  <Font name="Serif Regular" size="15.0" />
               </font>
            </Button>
            <Button fx:id="deleteRequestButton" layoutX="48.0" layoutY="267.0" mnemonicParsing="false" onAction="#deleteRequestButtonOnAction" prefHeight="33.0" prefWidth="231.0" style="-fx-background-color: #CAF4F4; -fx-background-radius: 30 30 30 30;" text="Delete Friend Request" textFill="#0b068f">
               <font>
                  <Font name="Serif Regular" size="15.0" />
               </font>
            </Button>
            <ImageView fx:id="friendsImage" fitHeight="150.0" fitWidth="183.0" layoutX="89.0" layoutY="63.0" pickOnBounds="true" preserveRatio="true">
