          <TableColumn fx:id="fromColumn" prefWidth="240.0" text="from" />
          <TableColumn fx:id="statusColumn" prefWidth="316.0" text="status" />
        </columns>
      </TableView>
      <Label fx:id="yourUsernameLabel" layoutX="28.0" layoutY="26.0" text="Label" textFill="#07c6c6" />
   </children>
</AnchorPane>
