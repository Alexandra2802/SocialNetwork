            <Button fx:id="backButton" layoutX="14.0" layoutY="247.0" mnemonicParsing="false" onAction="#leaveButtonOnAction" prefHeight="28.0" prefWidth="172.0" style="-fx-background-color: #CAF4F4; -fx-background-radius: 30 30 30 30;" text="Back" textFill="#0b068f">
               <font>
                  <Font name="Serif Regular" size="15.0" />
               </font>
            </Button>
            <Button fx:id="acceptRequest" layoutX="14.0" layoutY="120.0" mnemonicParsing="false" onAction="#goToRequestWindow" prefHeight="28.0" prefWidth="172.0" style="-fx-background-color: #CAF4F4; -fx-background-radius: 30 30 30 30;" text="Accept request" textFill="#0b068f">
               <font>
                  <Font name="Serif Regular" size="15.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
      <Label layoutX="414.0" layoutY="43.0" text="Friend Requests" textFill="#0b068f">
